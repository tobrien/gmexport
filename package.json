{
    "name": "@tobrien/gmexport",
    "version": "0.0.8",
    "description": "Export GMail",
    "main": "dist/main.js",
    "type": "module",
    "bin": {
        "gmexport": "./dist/src/main.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/main.js",
        "dev": "tsc -w",
        "watch": "nodemon --watch src --ext ts,json --exec \"yarn build\"",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "keywords": [
        "gmail",
        "export",
        "gmail-export"
    ],
    "author": "Tim O'Brien <tobrien@discursive.com>",
    "license": "Apache-2.0",
    "dependencies": {
        "@google-cloud/local-auth": "^3.0.1",
        "@types/commander": "^2.12.5",
        "@types/yaml": "^1.9.7",
        "commander": "^13.1.0",
        "dayjs": "^1.11.13",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "gaxios": "^6.7.1",
        "glob": "^11.0.1",
        "google-auth-library": "^9.6.3",
        "googleapis": "^148.0.0",
        "js-yaml": "^4.1.0",
        "luxon": "^3.5.0",
        "turndown": "^7.1.2",
        "winston": "^3.17.0",
        "yaml": "^2.7.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.23.0",
        "@types/jest": "^29.5.14",
        "@types/js-yaml": "^4.0.9",
        "@types/luxon": "^3.4.2",
        "@types/node": "^22.13.14",
        "@types/turndown": "^5.0.5",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "concurrently": "^9.1.2",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "ts-jest": "^29.2.6",
        "typescript": "^5.3.3"
    }
}
